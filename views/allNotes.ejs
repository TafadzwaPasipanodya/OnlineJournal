<% include chunks/head %>
<% include chunks/StuInfo %>
<% include chunks/nav_bar %>
<% include chunks/noteMenu %>

<table>
    <% notes.forEach(function(note) { %>
        <tr>
            <td><big><b><%= note.name %></b></big>
            <p><small><%= note.content%></small></p>
            <form method="post" action="/delete">
                <input type="hidden" name="itemid" value="<%= note._id %>">
                <input type="submit" value="Delete">
            </form>
            </td>
            <td><a href=“/note/<%= note._id %>”><%= note.name %></a>
            <input type="submit" value="Delete"></td>
            <td>
                <a href="/note/<%= note._id %>"><%= note.name %></a>
                <input type="submit" value="Delete">
            </td>
        </tr>
    <% }); %>
</table>

<% include chunks/foot %>